{
    "name": "pgs-soft/elastic-om",
    "type": "library",
    "description": "Elasticsearch Object Mapper",
    "keywords": [ "php", "elasticsearch", "object mapper" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomasz Brzeziński",
            "email": "tbrzezinski@pgs-soft.com"
        },
        {
            "name": "Kuba Werłos",
            "email": "jwerlos@pgs-soft.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pgs\\ElasticOM\\": "src/"
        }
    },
    "autoload-dev": {
